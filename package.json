{
  "name": "translate_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --ignorePath .gitignore --watch",
    "build": "pathpida --ignorePath .gitignore && next build",
    "start": "next start",
    "lint": "run-p -c lint:*",
    "lint:eslint": "eslint src --cache --ext .js,jsx,.ts,.tsx",
    "lint:prettier": "prettier --check --ignore-path .gitignore './src/**/*.{js,jsx,ts,tsx,json,css}'",
    "fix": "run-s -c fix:eslint fix:prettier",
    "fix:eslint": "eslint src  --cache --ext .js,jsx,.ts,.tsx --fix",
    "fix:prettier": "prettier --write --ignore-path .gitignore './src/**/*.{js,jsx,ts,tsx,json,css}'"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@hookform/resolvers": "^2.9.1",
    "@mantine/core": "^5.9.2",
    "@mantine/hooks": "^5.9.2",
    "@mantine/modals": "^5.9.2",
    "@mantine/next": "^5.9.2",
    "@tanstack/react-query": "^4.0.10",
    "@tanstack/react-query-devtools": "^4.0.10",
    "@types/react-cropper": "^1.3.2",
    "next": "^13.0.6",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.9.2",
    "@next/eslint-plugin-next": "^12.1.4",
    "@types/axios": "^0.14.0",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.18",
    "@types/react-dom": "17.0.14",
    "@types/styled-components": "^5.1.24",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "autoprefixer": "^10.4.13",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.0.6",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sort-keys-custom-order": "^1.0.5",
    "eslint-plugin-tailwindcss": "^3.8.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "lint-staged": "^12.3.7",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.20.1",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.6.3"
  }
}
